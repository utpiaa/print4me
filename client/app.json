{
  "expo": {
    "name": "Print4me",
    "slug": "print4me",
    "scheme": "print4me",
    "version": "1.0.0",
    "sdkVersion": "54.0.0",
    "orientation": "portrait",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#1976d2"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.abdelhaiattia.print4me"
    },
    "android": {
      "package": "com.abdelhaiattia.print4me",
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#1976d2"
      }
    },
    "web": {
      "bundler": "metro"
    },
    "extra": {
      "apiBaseUrl": "https://print4me.onrender.com",
      "eas": {
        "projectId": "a8ffded1-3444-4fbb-915a-8771ff63a093"
      }
    }
  }
}
